type Scope {
    id: ID! @id
    creator: User! @relation(
        name: "ScopeOfUser"
        link: INLINE
    )
    title: String!
    problems: [Problem!]! @relation(name: "ProblemOfScope")
    isSorted: Boolean!
    participants: [Participant!]! @relation(name: "ParticipantOfScope")
    isLinear: Boolean!
    skippable: Int
    isContest: Boolean!
    description: String!
    visible: Boolean!
    from: DateTime
    to: DateTime
    createdAt: DateTime! @createdAt
    updatedAt: DateTime! @updatedAt
}
